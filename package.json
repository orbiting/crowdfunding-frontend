{
  "name": "crowdfunding",
  "version": "0.0.1",
  "engines": {
    "node": "7.7.4"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon -w index.js -w constants.js -w server index.js",
    "build": "next build",
    "start": "node index.js",
    "precommit": "lint-staged",
    "standardize": "standard --fix 'components/**/*.js' 'lib/**/*.js' 'pages/**/*.js'",
    "heroku-postbuild": "npm run build",
    "translations": "gsheets --key=1OHoiENwJH-tqf9yXfXHOevAn17daO_CvB21Rym-oiqk --title=live --pretty --out lib/translations.json",
    "test": "wdio test/wdio.conf.js",
    "klog": "find . -name '*.js' -not -path \"./node_modules*\" -not -path \"./.next*\" | xargs wc -l",
    "upload-assets": "s3cmd sync --exclude=static/subtitles/ --verbose --acl-public -c ./.s3cfg static/ s3://republik-staging/cf_gui/static/",
    "upload-subtitles": "s3cmd sync --no-mime-magic -m text/vtt --verbose --acl-public -c ./.s3cfg static/subtitles/ s3://republik-staging/cf_gui/static/subtitles/",
    "upload-assets-production": "s3cmd sync --exclude=static/subtitles/ --verbose --acl-public -c ./.s3cfg static/ s3://republik/cf_gui/static/",
    "upload-subtitles-production": "s3cmd sync --no-mime-magic -m text/vtt --verbose --acl-public -c ./.s3cfg static/subtitles/ s3://republik/cf_gui/static/subtitles/"
  },
  "lint-staged": {
    "*.js": [
      "standard --fix",
      "git add"
    ]
  },
  "babel": {
    "presets": [
      "next/babel"
    ],
    "plugins": [
      "markdown-in-js/babel"
    ]
  },
  "dependencies": {
    "@project-r/styleguide": "^0.4.9",
    "apollo-client": "1.0.3",
    "body-parser": "^1.17.1",
    "cookie": "^0.3.1",
    "core-js": "^2.4.1",
    "cross-env": "^3.2.4",
    "d3-array": "^1.1.1",
    "d3-collection": "^1.0.3",
    "d3-color": "https://registry.npmjs.org/d3-color/-/d3-color-1.0.3.tgz",
    "d3-ease": "^1.0.3",
    "d3-format": "^1.1.1",
    "d3-geo": "^1.6.3",
    "d3-interpolate": "^1.1.4",
    "d3-scale": "^1.0.6",
    "d3-time": "^1.0.6",
    "d3-time-format": "^2.0.5",
    "d3-timer": "^1.0.5",
    "email-validator": "^1.0.7",
    "express": "^4.15.2",
    "express-basic-auth": "^1.0.1",
    "glamor": "^2.20.24",
    "graphql": "^0.9.1",
    "isomorphic-unfetch": "^1.0.0",
    "load-script": "^1.0.0",
    "markdown-in-js": "^1.1.3",
    "marked": "^0.3.6",
    "next": "^2.4.6",
    "prop-types": "^15.5.7",
    "react": "^15.5.4",
    "react-apollo": "^1.1.3",
    "react-maskedinput": "^3.3.4",
    "react-textarea-autosize": "^4.3.0",
    "redux": "^3.6.0",
    "topojson-client": "^3.0.0",
    "unfetch": "^2.1.2"
  },
  "devDependencies": {
    "d3-dsv": "https://registry.npmjs.org/d3-dsv/-/d3-dsv-1.0.5.tgz",
    "dotenv": "^4.0.0",
    "gsheets": "^2.0.0",
    "husky": "^0.13.2",
    "lint-staged": "^3.4.0",
    "nodemon": "^1.11.0",
    "standard": "^9.0.2",
    "wdio-dot-reporter": "0.0.8",
    "wdio-jasmine-framework": "^0.3.0",
    "wdio-sauce-service": "^0.3.1",
    "wdio-spec-reporter": "^0.1.0",
    "webdriverio": "^4.6.2"
  }
}
